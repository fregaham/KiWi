<?xml version="1.0" encoding="UTF-8"?>
<pages xmlns="http://jboss.com/products/seam/pages"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd"

       no-conversation-view-id="/home.xhtml"
       login-view-id="/login.xhtml">

	<page view-id="/rrs/home.xhtml" >
        <navigation from-action="upload">
            <redirect view-id="/rrs/upload.xhtml"/>
        </navigation>
        
         <navigation from-action="annotate">
            <redirect view-id="/rrs/annotate.xhtml"/>
        </navigation>
    </page>
    
    <page view-id="/rrs/upload.xhtml" >
        <navigation from-action="#{rrs.importAction.doImport}">
            <redirect view-id="/rrs/upload_edit.xhtml"/>
        </navigation>
        <navigation from-action="#{rrs.importAction.doImportXml}">
            <redirect view-id="/rrs/home.xhtml"/>
        </navigation>
    </page>
    
